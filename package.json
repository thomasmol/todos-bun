{
  "name": "todos-bun",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:migrate": "vite build && bun src/lib/server/database/migrate.ts",
    "preview": "vite preview",
    "start": "bun build/index.js",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "generate": "drizzle-kit generate:pg",
    "push": "drizzle-kit push:pg",
    "pull": "drizzle-kit pull:pg",
    "migrate": "bun src/lib/server/database/migrate.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "autoprefixer": "^10.4.16",
    "drizzle-kit": "^0.19.13",
    "nodemailer": "^6.9.6",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "svelte": "^4.2.2",
    "svelte-adapter-bun": "^0.5.0",
    "svelte-adapter-bun-thomas": "./svelte-adapter-bun-thomas",
    "svelte-check": "^3.4.3",
    "ts-node": "^10.9.1",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.4.1",
    "tsx": "^3.12.7",
    "typescript": "^5.2.2",
    "vite": "^4.4.2"
  },
  "type": "module",
  "dependencies": {
    "@auth/core": "^0.*",
    "@auth/drizzle-adapter": "^0.*",
    "@auth/sveltekit": "^0.*",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.*",
    "postgres": "^3.4.0"
  },
  "overrides": {
    "@auth/core": "$@auth/core"
  }
}
